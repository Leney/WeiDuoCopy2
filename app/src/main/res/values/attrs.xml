<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="PointView">
        <attr name="text" format="string" />
        <attr name="src" format="reference" />
    </declare-styleable>
    <!-- 群聊头像自定义属性   分割线-->
    <declare-styleable name="NineGridImageView">
        <attr name="imgGap" format="dimension" />
    </declare-styleable>

    <!--自定义圆形图片属性-->
    <declare-styleable name="CircleImageView">
        <attr format="dimension" name="civ_border_width"/>
        <attr format="color" name="civ_border_color"/>
        <attr format="boolean" name="civ_border_overlay"/>
        <attr format="color" name="civ_fill_color"/>
    </declare-styleable>

    <!-- 跑马灯效-->
    <declare-styleable name="MarqueeView">
        <attr name="drawableLeft" format="reference" />
        <attr name="drawablePadding" format="dimension" />
        <attr name="textSize" format="dimension" />
        <attr name="textColor" format="color" />
        <!--数值越小越快-->
        <attr name="speed" format="integer" />
        <attr name="scrollText" format="string" />
    </declare-styleable>

    <declare-styleable name="KeRatingBarView">

        <attr name="starImageSize" format="dimension"/>
        <attr name="starCount" format="integer"/>
        <attr name="starEmpty" format="reference"/>
        <attr name="starFill" format="reference"/>

    </declare-styleable>
</resources>
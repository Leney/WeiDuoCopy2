    <style name="hardware">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#333333</item>
        <item name="android:textSize">16sp</item>
    </style>

    <declare-styleable name="MySeekBar">
        <attr name="top_text" format="string"/>
    </declare-styleable>

    <style name="hardware_line">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">0.3dp</item>
        <item name="android:background">#f0f0f0</item>
    </style>
    <!-- 硬件管理中seekbar的样式-->
    <style name="jrt_seekbar_style" parent="Base.Widget.AppCompat.SeekBar">
        <item name="android:progressDrawable">@drawable/my_seekbar_style</item>
        <item name="android:thumb">@drawable/seekbar_bg</item>
    </style>

    <!--头像选择-->
    <style name="profile_picture_dialog_style" parent="android:style/Theme.Dialog">
        <item name="android:background">@android:color/white</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:windowNoTitle">true</item>
    </style>

    <!--地址选择框主题-->
    <style name="home_address_dialog_style" parent="android:style/Theme.Dialog">
        <item name="android:background">@android:color/white</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowNoTitle">true</item>
    </style>

    <style name="dialog_no_title_style" parent="android:style/Theme.Dialog">
        <item name="android:background">@android:color/white</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowNoTitle">true</item>
    </style>


    <style name="TranslucentTheme" parent="AppTheme.Base">

    </style>

    <style name="style_grow_up_main_btn_selector">
        <item name="android:textColor">#f1f2f3</item>
        <item name="android:textSize">12sp</item>
        <item name="android:gravity">center</item>
        <item name="android:button">@null</item>
        <item name="android:background">@drawable/selector_grow_up_main</item>
        <item name="android:layout_width">30dp</item>
        <item name="android:layout_height">30dp</item>

    </style>

</resources>

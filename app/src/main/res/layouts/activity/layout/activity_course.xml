<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <com.shuyu.gsyvideoplayer.video.NormalGSYVideoPlayer
        android:id="@+id/web_player"
        android:layout_width="match_parent"
        android:layout_height="@dimen/space_147" />

    <com.zhongke.weiduo.mvp.ui.widget.layout.TabItemChangeLayout
        android:id="@+id/tab_item_layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

    </com.zhongke.weiduo.mvp.ui.widget.layout.TabItemChangeLayout>

    <android.support.v4.view.ViewPager
        android:id="@+id/viewpager"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

    </android.support.v4.view.ViewPager>
</LinearLayout>